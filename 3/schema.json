{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
        "value"
    ],
    "properties": {
        "value": {
            "anyOf": [{
                    "type": "number",
                    "minimum": 1,
                    "maximum": 10
                },
                {
                    "type": "number",
                    "minimum": 20,
                    "maximum": 30,
                    "exclusiveMinimum": 20
                }
            ]
        }
    }
}